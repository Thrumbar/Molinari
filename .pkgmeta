package-as: Molinari

externals:
    embeds/LibStub:
        url: svn://svn.wowace.com/wow/libstub/mainline/trunk
    embeds/LibProcessable:
        url: git://git.curseforge.net/wow/libprocessable/mainline.git
        tag: latest

optional-dependencies:
    - libstub
    - libprocessable

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

ignore:
    - .gitmodules
    - README.md
    - CONTRIBUTING.md
    - embeds/LibStub/tests
    - embeds/LibStub/.pkgmeta
    - embeds/LibProcessable/.gitmodules
    - embeds/LibProcessable/.pkgmeta
    - embeds/LibProcessable/README.md
    - embeds/LibProcessable/CONTRIBUTING.md
